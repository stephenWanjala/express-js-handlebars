<div class="row">
<div class="col-6" ><h1>Users</h1></div>
<div class="col-6 ">
  <a href="/addUser" type="button" class="btn btn-primary btn-outlned ">+ Add new User</a>
</div>

</div>

<table class="table table-bordered">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Email</th>
      <th scope="col">phone</th>
      <th scope="col " class="text-end">Action</th>
    </tr>
  </thead>
  <tbody>
    {{#each rows}}
    <tr>
      <th scope="row">{{this.id}}</th>
      <td>{{this.first_name}} </td>
      <td>{{this.last_name}}</td>
      <td>{{this.phone}}</td>
      <td>{{this.email}}</td>
      <td class="text-end">
          <a href="#" type="button" class="btn btn-light btn-small"><i class="bi bi-eye"></i>View</a>
      <a href="#" type="button" class="btn btn-light btn-small"><i class="bi bi-pencil"></i>Edit</a>
      <a href="#" type="button" class="btn btn-light btn-small"><i class="bi bi-person-x"></i>Delete</a>
      </td>
    </tr>
    
      
    {{/each}}
  </tbody>
</table>